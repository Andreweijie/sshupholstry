    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "flwww": "^2.0.15",
    "nodemailer": "^6.4.2",
    "react-photo-gallery": "^8.0.0",
    "react-slick": "^0.25.2"
  }
